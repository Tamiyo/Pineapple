#add(a, b) {
    return a + b;
}

#sub(a, b) {
    return a - b;
}

#mul(a, b) {
    return a * b;
}

#add4(a, b) {
    x = 1 + 3;
    return a + b + x;
}

#main() {
    x = add(2, 7);
    y = sub(5, 3);
    z = mul(3, 7);
    q = div(6, 5);
    k = add4(2, 3);

    print(x, y, z, q, k);
}

#div(a, b) {
    return a / b;
}